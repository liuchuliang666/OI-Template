            "constexpr int inv(int x) { return qpow(x, P - 2); }",
            "void init(int N) {",
            "    fac[0] = 1;",
            "    rep(i, 1, N) fac[i] = fac[i - 1] * i % P;",
            "    ifac[N] = inv(fac[N]);",
            "    per(i, N - 1, 0) ifac[i] = ifac[i + 1] * (i + 1) % P;",
            "}",
            "int C(int n, int m) { return n < m || m < 0 ? 0 : fac[n] * ifac[m] % P * ifac[n - m] % P; }",
