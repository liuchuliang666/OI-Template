    "matrix": {
        "prefix": "```mat",
        "body": [
            "struct Mat {",
            "    int n, m, val[MAXN][MAXN];",
            "    int *operator[](int x) { return val[x]; }",
            "    Mat(int n = 0, int m = 0) : n(n), m(m) {}",
            "    friend Mat operator*(Mat a, Mat b) {",
            "        Mat c{a.n, b.m};",
            "        // init",
            "        rep(i, 1, a.n) rep(k, 1, a.m) rep(j, 1, b.m) /* oper(c[i][j], a[i][k] ? b[k][j])*/;",
            "        return c;",
            "    }",
            "};",
            "Mat qpow(Mat a, int b) {",
            "    Mat res{a.n, a.m};",
            "    for (/* res.init() */; b; b >>= 1, a = a * a)",
            "        if (b & 1) res = res * a;",
            "    return res;",
            "}",
        ]
