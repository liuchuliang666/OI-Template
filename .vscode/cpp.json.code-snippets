            "#define chkmx(a, b) ((a) = max((a), (b)))",
            "#define chkmn(a, b) ((a) = min((a), (b)))",
            "#define mid (((l) + (r)) >> 1)",
            "#define ls ((x) << 1)",
            "#define rs ((x) << 1 | 1)",
            "#define lson ls, l, mid",
