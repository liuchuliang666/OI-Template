    "superfastio": {
        "prefix": "```sfio",
        "body": [
            "#if defined(ONLINE_JUDGE) || defined(CPH)",
            "struct FastIO {",
            "    static constexpr int siz = 1 << 15;",
            "    char inbuf[siz], oubuf[siz], *pa, *pb, *p;",
            "    FastIO() : inbuf({}), oubuf({}), pa(inbuf), pb(inbuf), p(oubuf) {}",
            "    ~FastIO() { (p != oubuf && fwrite(p = oubuf, 1, p - oubuf, stdout)); }",
            "    inline int gc() { return (pa == pb && (pb = (pa = inbuf) + fread(inbuf, 1, siz, stdin), pa == pb)) ? EOF : *(pa++); }",
            "    inline int pc(const char c) { return (p - oubuf == siz && fwrite(p = oubuf, 1, siz, stdout)), *p++ = c, 0; }",
            "} fastio;",
            "#define gc fastio.gc",
            "#define pc fastio.pc",
            "#elif defined(WIN32)",
            "#define gc getchar",
            "#define pc putchar",
            "#else",
            "#define gc getchar_unlocked",
            "#define pc putchar_unlocked",
            "#endif",
            "template <typename T = int>",
            "inline T rd() {",
            "#define isdigit(x) ((x) >= '0' && (x) <= '9')",
            "    static T x, f;",
            "    static char ch;",
            "    for (f = 1, ch = gc(); !isdigit(ch); ch = gc()) (ch != '-' || (f = -1));",
            "    for (x = 0; isdigit(ch); ch = gc()) x = (x << 3) + (x << 1) + (ch ^ 48);",
            "    return x * f;",
            "}",
            "template <typename T>",
            "inline void wt(T x) {",
            "    static signed num[100] = {}, top = 0;",
            "    (x >= 0 || (pc('-'), x = -x));",
            "    do num[++top] = x % 10;",
            "    while (x /= 10);",
            "    while (top) pc(num[top--] + '0');",
            "}",
            "template <typename... T> inline void rds(T &...x) { (void)(signed[]){(x = rd<T>(), 0)...}; }",
            "template <typename... T> inline void wts(const T &...x) { (void)(signed[]){(wt(x), pc(' '))...}, pc('\\n'); }",
        ],
        "description": "get super faster io"
    },
