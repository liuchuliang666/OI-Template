            "#define go(u, i) for (int i = head[u]; i; i = e[i].nxt)",
            "struct edge {",
            "    int v, w, nxt;",
            "} e[MAXN << 1];",
            "int head[MAXN];",
            "inline void addedge(int u, int v, int w) {",
            "    static int edgecnt = 0;",
            "    e[++edgecnt] = {v, w, head[u]};",
            "    head[u] = edgecnt;",
            "}",
            "inline void adde(int u, int v, int w) { addedge(u, v, w), addedge(v, u, w); }",
