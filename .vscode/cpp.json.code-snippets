    "qpow": {
        "prefix": "```qpow",
        "body": [
            "constexpr int qpow(int a, int b)",
            "{",
            "    int res = 1;",
            "    for (; b; b >>= 1, mul(a, a)) ((b & 1) && mul(res, a));",
            "    return res;",
            "}",
            "constexpr int inv(int x) { return qpow(x, P - 2); }",
        ]
    },
    "comb": {
        "prefix": "```comb",
        "body": [
            "int fac[MAXN], ifac[MAXN];",
            "constexpr int qpow(int a, int b) {",
            "    int res = 1;",
            "    for (; b; b >>= 1, mul(a, a)) ((b & 1) && mul(res, a));",
            "    return res;",
            "}",
            "constexpr int inv(int x) { return qpow(x, P - 2); }",
            "void init(int N) {",
            "    fac[0] = 1;",
            "    rep(i, 1, N) fac[i] = fac[i - 1] * i % P;",
            "    ifac[N] = inv(fac[N]);",
            "    per(i, N - 1, 0) ifac[i] = ifac[i + 1] * (i + 1) % P;",
            "}",
            "int C(int n, int m) { return n < m ? 0 : fac[n] * ifac[m] % P * ifac[n - m] % P; }",
            "int L(int n, int m) {",
            "    if (n < m) return 0;",
            "    int res = 1;",
            "    while (m) mul(res, C(n % P, m % P)), n /= P, m /= P;",
            "    return res;",
            "}",
        ]
    },
